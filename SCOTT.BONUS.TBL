DROP TABLE SCOTT.BONUS CASCADE CONSTRAINTS;

CREATE TABLE SCOTT.BONUS
(
  ENAME  VARCHAR2(10 BYTE),
  JOB    VARCHAR2(9 BYTE),
  SAL    NUMBER,
  COMM   NUMBER
)
